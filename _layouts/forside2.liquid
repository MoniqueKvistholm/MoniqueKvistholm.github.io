layout: default
---

<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>{{ page.title }}</title>
  <link rel="stylesheet" href="/assets/css/main.css">
  <style>
    .full-width-image {
      width: 100vw; /* Fyld hele vinduets bredde */
      height: 100vh; /* Fyld hele vinduets højde */
      object-fit: cover; /* Gør billedet til at fylde hele containeren uden at miste proportioner */
      object-position: center; /* Centrer billedet i forhold til containeren */
      display: block;
      margin: 0;
      padding: 0;
      position: fixed; /* Fastgør billedet, så det ikke påvirker layoutet */
      top: 0; /* Placer billedet øverst */
      left: 0; /* Placer billedet helt til venstre */
      z-index: -1; /* Sørg for, at billedet er bag andre elementer */
    }
  </style>
</head>
<body>
  <main>
    <img src="/assets/img/Forsidebillede.jpg.jpg" alt="example image" class="full-width-image">
  </main>
</body>
</html>
